<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>board_hy</display-name>
  <servlet>
    <servlet-name>IndexController</servlet-name>
    <servlet-class>com.board.controller.FrontController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>IndexController</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>FrontController</servlet-name>
    <servlet-class>com.board.controller.FrontController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FrontController</servlet-name>
    <url-pattern>*.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AjaxController</servlet-name>
    <servlet-class>com.board.controller.AjaxController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AjaxController</servlet-name>
    <url-pattern>*.ajax</url-pattern>
  </servlet-mapping>
</web-app>